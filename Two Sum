class Solution {
    public int[] twoSum(int[] nums, int target) {
       int sum=0;
       int a= nums.length;
        for(int i=0;i<a;i++)
        {
            for (int j = i + 1; j <a; j++) 
            {
               sum=nums[i]+nums[j];
               if(sum==target)
               {
                return new int[] { i, j};
                
                }
             }
        }    
        return null;
    }
}
